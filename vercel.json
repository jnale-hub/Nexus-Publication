{
  "builds": [
    {
      "src": "nexus/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "/static/(.*)",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles_build" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "nexus/wsgi.py"
    }
  ]
}
